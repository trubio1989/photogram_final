<!-- Form like start -->
<form action="/create_like_from_photo" method="post">
<!-- Form like end -->

  <!-- Label and input for fan_id start -->
  <div class="form-group">
    <label for="fan_id">
      Fan
    </label>

    <!-- Input for fan_id start -->
    <%= select_tag(:fan_id, options_from_collection_for_select(User.all, :id, :email, @like.fan_id), :class => "form-control") %>
    <!-- Input for fan_id end -->
  </div>
  <!-- Label and input for fan_id end -->

  <!-- Label and input for photo_id start -->
  <!-- Input for photo_id start -->
  <input type="hidden" name="photo_id" value="<%= @photo.id %>">
  <!-- Input for photo_id end -->

  <button class="btn btn-block btn-outline-secondary">
    Create like
  </button>
</form>
